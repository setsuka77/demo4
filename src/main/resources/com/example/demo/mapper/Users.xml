<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.UsersMapper">

	
	<select id="findUser" resultType="com.example.demo.entity.Users">
		SELECT * FROM users
		WHERE id =
		#{id}
	</select>
	
	<insert id="insert" parameterType="com.example.demo.entity.Users">
	INSERT INTO users(id,name,pass,email)
	VALUES(#{id},#{name},#{pass},#{email})
	</insert>

</mapper>